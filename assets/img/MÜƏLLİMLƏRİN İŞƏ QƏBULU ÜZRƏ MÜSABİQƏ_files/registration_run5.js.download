reg_run = {
    add_emek_file : function()
    {
        var uploader = document.getElementById('btnEmekYukle');

        upclick(
          {
           element: uploader,
           action: "upload_emek",
           onstart:
             function(filename)
             {
                 $('#imgAjaxLoader').show();
             },
           oncomplete:
             function(response_data)
             {

               //alert(response_data);
               var json_obj = JSON.parse(response_data);
               $("spEmekBax-"+json_obj.number).hide();
               if(parseInt(json_obj.code)==0)
               {
                   $("#spEmekBax").show();
                   $("#btnEmekBax").attr("href", json_obj.file_name);
                   $("#hdnEmek").val(json_obj.file_name);
                   //reg.SEBEB_FILE_UPLOADED=true;
               } else {
                   toastr.error(json_obj.message);
               }
               // else if(parseInt(json_obj.code)==1)
               // {
               //     toastr.error("Əlavə etdiyin file formatını uyğun seçməmisiniz");
               // }
               // else if(parseInt(json_obj.code)==2)
               // {
               //     toastr.error("Əlavə etdiyiniz faylın ölçüsü böyükdür . Ölçü "+parseFloat(json_obj.file_size/1024/1024).toFixed(2)+' MB');
               // }

               $('#imgAjaxLoader').hide();

             }
          });
    },
    add_visual_impairment : function()
    {
        var uploader = document.getElementById('btn_visual_impairment');

        upclick(
            {
                element: uploader,
                action: "upload_visual_impairment",
                onstart:
                    function(filename)
                    {
                        $('#imgAjaxLoader').show();
                    },
                oncomplete:
                    function(response_data)
                    {

                        //alert(response_data);
                        let json_obj = JSON.parse(response_data);
                        console.log(json_obj);
                        $("spEmekBax-"+json_obj.number).hide();
                        if(parseInt(json_obj.code)==0)
                        {
                            $("#sp_visual_impairment").show();
                            $("#btnGozBax").attr("href", json_obj.file_name);
                            $("#hdn_visual_impairment").val(json_obj.file_name);
                        }
                        else
                        {
                            toastr.error(json_obj.message);
                            // toastr.error("Əlavə etdiyin file formatını uyğun seçməmisiniz");
                            // toastr.error("Əlavə etdiyiniz faylın ölçüsü böyükdür . Ölçü "+parseFloat(json_obj.file_size/1024/1024).toFixed(2)+' MB');
                        }
                        $('#imgAjaxLoader').hide();
                    }
            });
    },
    add_diplom_new : function()
    {
        var uploader = document.getElementById('btnDiplomYukle');

        upclick(
            {
                element: uploader,
                action: 'upload_diplom',
                onstart:
                    function(filename)
                    {
                        $('#imgAjaxLoader').show();
                    },
                oncomplete:
                    function(response_data)
                    {
                        //alert(response_data);
                        var json_obj = JSON.parse(response_data);
                        $("spDiplomBax-"+json_obj.number).hide();
                        if(parseInt(json_obj.code)==0)
                        {
                            $("#spDiplomBax").show();
                            $("#btnDiplomBax").attr("href", json_obj.file_name);
                            $("#hdnDiplom").val(json_obj.file_name);
                            //reg.SEBEB_FILE_UPLOADED=true;
                        }
                        else if(parseInt(json_obj.code)==1)
                        {
                            toastr.error("Əlavə etdiyin file formatını uyğun seçməmisiniz");
                        }
                        else if(parseInt(json_obj.code)==2)
                        {
                            toastr.error("Əlavə etdiyiniz faylın ölçüsü böyükdür . Ölçü "+parseFloat(json_obj.file_size/1024/1024).toFixed(2)+' MB');
                        }

                        $('#imgAjaxLoader').hide();

                    }
            });



    },
    add_sebeb_files : function()
    {
        var uploader = document.getElementById('btnSebeb');

        upclick(
            {
                element: uploader,
                action: "upload_sebeb?f=0",
                onstart:
                    function(filename)
                    {
                        $('#imgAjaxLoader').show();
                    },
                oncomplete:
                    function(response_data)
                    {

                        //alert(response_data);
                        var json_obj = JSON.parse(response_data);
                        $("divSebebBax-"+json_obj.number).hide();
                        if(parseInt(json_obj.code)==0)
                        {
                            $('#chkMyfile-'+json_obj.number).prop('checked', true);
                            $("#btnIsSebebFile-"+json_obj.number).val("1");
                            $("#divSebebBax-"+json_obj.number).show();
                            $("#btnSebebBax-"+json_obj.number).attr("href", json_obj.file_name);
                            reg.SEBEB_FILE_UPLOADED=true;
                        }
                        else if(parseInt(json_obj.code)==1)
                        {
                            toastr.error("Əlavə etdiyin file formatını uyğun seçməmisiniz");
                        }
                        else if(parseInt(json_obj.code)==2)
                        {
                            toastr.error("Əlavə etdiyiniz faylın ölçüsü böyükdür . Ölçü "+parseFloat(json_obj.file_size/1024/1024).toFixed(2)+' MB');
                        }
    
                        $('#imgAjaxLoader').hide();

                    }
            });



    }
}
if(document.getElementById('btnEmekYukle')) {
    reg_run.add_emek_file();
}

if(document.getElementById('btnDiplomYukle')) {
    reg_run.add_diplom_new();
}

if(document.getElementById('btn_visual_impairment')) {
    reg_run.add_visual_impairment();
}

if(document.getElementById('btnSebeb')) {
    reg_run.add_sebeb_files();
}

